[Unit]
Description=Nonlinear-Labs C15 Audio Engine
After=syslog.target systemd-modules-load.service sound.target

[Service]
WorkingDirectory=@CMAKE_INSTALL_PREFIX@/nonlinear/audio-engine
ExecStart=@CMAKE_INSTALL_PREFIX@/nonlinear/audio-engine/audio-engine -m EMPHASE -a PCH -n 3 -s 49
Restart=on-failure
RestartSec=1

[Install]
WantedBy=multi-user.target
