{
  "layouts": {
    "PresetSelectionForVoiceGroupLayout": {
      "LocalEventProvider" : "PresetSelectionForVoiceGroup",
      "Selector": {
        "UIFocus": "Sound",
        "UIMode": "Edit",
        "UIDetail": "SoundSelectPresetForVoiceGroup"
      },
      "Controls": {
        "ButtonA": {
          "Class": "Button",
          "Position": "BUTTON_A",
          "Init": {
            "Text[Text]": "Back"
          }
        },
        "DirectLoadButton": {
          "Class": "Button",
          "Position": "BUTTON_D",
          "Init": {
            "Text[Text]": "Direct Load"
          },
          "Events": "DirectLoadStatus => ANY[Highlight]"
        },
        "PreviousPreset": {
          "Class": "Preset",
          "Position": "64, 12",
          "Events": "PreviousNumber => Number[Text],PreviousName => Name[Text]"
        },
        "CurrentPreset": {
          "Class": "Preset",
          "Position": "64, 24",
          "Events": "CurrentNumber => Number[Text],CurrentName => Name[Text]"

        },
        "NextPreset": {
          "Class": "Preset",
          "Position": "64, 36",
          "Events": "NextNumber => Number[Text],NextName => Name[Text]"
        },
        "BankLabel": {
          "Class": "WideInvertedLabel",
          "Position": "64, 0",
          "Events": "PresetListBankName => Text[Text]"
        },
        "LeftButton": {
          "Class": "ArrowLeft",
          "Position": "65, 1",
          "Events": "PresetListHasLeftBank => ANY[Highlight]"
        },
        "RightButton": {
          "Class": "ArrowRight",
          "Position": "184, 1",
          "Events": "PresetListHasRightBank => ANY[Highlight]"
        }
      },
      "EventSinks": {
        "BUTTON_A": "SwitchToSoundFocus",
        "BUTTON_D": "ToggleDirectLoad",
        "BUTTON_B": "DecBankSelectionPresetList",
        "BUTTON_C": "IncBankSelectionPresetList",
        "BUTTON_INC": "IncPresetSelectionPresetList",
        "BUTTON_DEC": "DecPresetSelectionPresetList",
        "BUTTON_ENTER": "DoPresetListAction"
      }
    }
  }
}
