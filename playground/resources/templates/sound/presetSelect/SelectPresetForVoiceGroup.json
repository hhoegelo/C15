{
  "layouts": {
    "PresetSelectionForVoiceGroupLayout": {
      "EventProvider" : "IndependentPresetSelectionEvents",
      "Selector": {
        "UIFocus": "Sound",
        "UIMode": "Edit",
        "UIDetail": "SoundSelectPresetForVoiceGroup"
      },
      "Controls": {
        "ButtonA": {
          "Class": "Button",
          "Position": "BUTTON_A",
          "Init": {
            "Text[Text]": "Back"
          }
        },
        "PreviousPreset": {
          "Class": "Preset",
          "Position": "64, 12",
          "Events": "PreviousNumber => Number[Text],PreviousName => Name[Text]"
        },
        "CurrentPreset": {
          "Class": "Preset",
          "Position": "64, 29",
          "Events": "CurrentNumber => Number[Text],CurrentName => Name[Text]"
        },
        "NextPreset": {
          "Class": "Preset",
          "Position": "64, 46",
          "Events": "NextNumber => Number[Text],NextName => Name[Text]"
        },
        "BankLabel": {
          "Class": "WideInvertedLabel",
          "Position": "64, 0",
          "Events": "PresetListBankName => Text[Text]"
        },
        "LeftButton": {
          "Class": "ArrowLeft",
          "Position": "65, 1",
          "Events": "CanLeft => ANY[Highlight]"
        },
        "RightButton": {
          "Class": "ArrowRight",
          "Position": "184, 1",
          "Events": "CanRight => ANY[Highlight]"
        }
      },
      "EventSinks": {
        "BUTTON_A": "SwitchToSoundFocus",
        "BUTTON_B": "Left",
        "BUTTON_C": "Right",
        "BUTTON_INC": "Up",
        "BUTTON_DEC": "Down",
        "BUTTON_ENTER": "DoPresetListAction"
      }
    }
  },
  "styles" : {
      "SelectPresetTextStyle" : {
          "selector": {
            "ControlClasses": "Preset",
            "PrimitiveClasses": "Text",
            "ControlInstances": "CurrentPreset"
          },
          "styles": {
            "BackgroundColor": "C103",
            "Color": "C43"
          }
      }
    }
}
